<h1>Search!</h1>


<%= form_with url: "/search", method: :get do |form| %>

  <%= form.text_field :search %>

  <%= form.submit "Search" %>
<% end %>
<% @results.each do |movie| %>
  <%= image_tag "#{@small_poster}#{movie["poster_path"]}" unless movie["poster_path"].nil? %>
  <%= movie["original_title"] %><br />
<% end %>


